<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mkyong.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        	  android:minSdkVersion="14"
       		  android:targetSdkVersion="17"  />
    
    <uses-permission android:name="android.permission.INTERNET" /> 
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name="com.banana.locations.Main_Activity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.banana.locations.Messagerecord"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.Messagerecord" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.banana.locations.Notifyalert_Activity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.Notifyalert_Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.banana.locations.List_Activity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.List_Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.banana.locations.Settings"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.Settings" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <service android:name="com.banana.service.LocationService" />
       
    </application>
    


</manifest>